{
  "name": "dead",
  "version": "0.0.2",
  "description": "testing",
  "main": "./dist/index.js",
  "files": [
    "/dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && babel src -d dist --copy-files",
    "dev": "babel src -d dist --copy-files --watch",
    "prepublish": "rm -rf dist && babel src -d dist --copy-files"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.14.3",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-jsx": "^3.2.1"
  },
  "peerDependencies": {
    "react": "^16.8",
    "react-dom": "^16.8"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "impliedStrict": true,
        "experimentalObjectRestSpread": true
      },
      "allowImportExportEverywhere": true
    },
    "plugins": [
      "react",
      "import"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "func-names": [
        "error",
        "as-needed"
      ],
      "no-shadow": "error",
      "import/no-mutable-exports": "error",
      "import/no-anonymous-default-export": [
        "error",
        {
          "allowArray": true,
          "allowArrowFunction": true,
          "allowLiteral": true,
          "allowObject": true
        }
      ],
      "no-extra-semi": 0,
      "react/prop-types": 0,
      "react/no-unescaped-entities": 0,
      "react/jsx-no-target-blank": 0,
      "react/no-string-refs": 0
    }
  },
  "eslintIgnore": [
    "**/node_modules/**/*.js",
    "**/.next/**/*.js"
  ]
}
